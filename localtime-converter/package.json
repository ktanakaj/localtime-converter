{
  "name": "localtime-converter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean-app && browserify ./app/scripts/app.js -t babelify -t browserify-ngannotate -o ./app/bundle.js",
    "watch": "npm run clean-app && watchify --poll=true ./app/scripts/app.js -t babelify -t browserify-ngannotate -v -d -o ./app/bundle.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive ./test",
    "doc": "npm run clean-doc && jsdoc -r ./package.json ./app/scripts ./test",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -f ./app/bundle.js",
    "clean-doc": "rm -rf ./out",
    "eslint": "eslint ./app/scripts ./test"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-ngannotate": "^2.0.0",
    "eslint": "^3.4.0",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "presets": [
          "power-assert"
        ]
      }
    }
  }
}
