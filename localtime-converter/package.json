{
  "name": "localtime-converter",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "build": "npm run clean-app && webpack --optimize-minimize",
    "watch": "npm run clean-app && webpack --watch --devtool source-map",
    "test": "NODE_ENV=test mocha --compilers ts:espower-typescript/guess --recursive ./test",
    "doc": "npm run clean-doc && typedoc ./src/app ./test --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -f ./src/bundle.js",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint ./src/app ./test"
  },
  "author": "Koichi Tanaka",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "moment": "^2.17.0",
    "moment-timezone": "^0.5.10",
    "ng2-translate": "^4.0.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/moment-timezone": "^0.2.32",
    "@types/power-assert": "^1.4.29",
    "core-js": "^2.4.1",
    "espower-typescript": "^6.0.1",
    "mocha": "^3.1.2",
    "power-assert": "^1.4.2",
    "ts-loader": "^1.2.2",
    "tslint": "^4.0.1",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.10",
    "webpack": "^1.13.3"
  }
}
