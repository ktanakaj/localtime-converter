{
  "name": "localtime-converter",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "build": "npm run clean-app && webpack --optimize-minimize",
    "watch": "npm run clean-app && webpack --watch --devtool source-map",
    "test": "NODE_ENV=test mocha --compilers ts:espower-typescript/guess --recursive ./test",
    "doc": "npm run clean-doc && typedoc ./public/app ./test --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -f ./public/bundle.js ./public/bundle.js.map",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint ./public/app ./test"
  },
  "author": "Koichi Tanaka",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@types/moment-timezone": "^0.2.34",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "ng2-translate": "^5.0.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.39",
    "@types/power-assert": "^1.4.29",
    "core-js": "^2.4.1",
    "espower-typescript": "^8.0.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "ts-loader": "^2.0.1",
    "tslint": "^4.5.1",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  }
}
